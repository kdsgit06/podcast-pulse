{
  "version": 2,
  "builds": [
    {
      "src": "api/download.py",
      "use": "@vercel/python"
    },
    {
      "src": "build/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/download",
      "dest": "api/download.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}