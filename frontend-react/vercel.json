{
  "version": 2,
  "builds": [
    {
      "src": "frontend-react/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/frontend-react/index.html"
    }
  ],
  "framework": "create-react-app",
  "installCommand": "cd frontend-react && npm install",
  "buildCommand": "cd frontend-react && npm run build",
  "outputDirectory": "frontend-react/build"
}