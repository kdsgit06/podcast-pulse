{
  "version": 2,
  "builds": [
    {
      "src": "backend/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/download",
      "dest": "backend/main.py"
    },
    {
      "src": "/history",
      "dest": "backend/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend-react/index.html"
    }
  ],
  "env": {
    "APP_MODULE": "backend.main:app"
  }
}